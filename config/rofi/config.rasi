configuration {
	display-drun: "Applications";
	modes: "window,drun,run,ssh";
	font: "JetBrainsMono NF 12";
	icon-theme: "Papirus";
	timeout {
      action: "kb-cancel";
      delay:  0;
  	}
  	filebrowser {
    	directories-first: true;
    	sorting-method:    "name";
  	}
  	icon {
    	theme: "Papirus";
  	}
}
@theme "/usr/share/rofi/themes/marv.rasi"
